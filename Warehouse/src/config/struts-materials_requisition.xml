<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="requisitionActions" extends="struts-default">

		<action name="add_MaterialsRequisition" class="materialsRequisitionAction"
			method="add">
			<result name="input">/addMaterialsRequisition.jsp</result>
			<result name="success">/addMaterialsRequisition.jsp</result>
		</action>

		<action name="delete_MaterialsRequisition" class="materialsRequisitionAction"
			method="delete">
			<result name="success">/thankyou.jsp</result>
		</action>

		<action name="select_MaterialsRequisition" class="materialsRequisitionAction"
			method="select">
			<result name="input">/selectMaterialsRequisition.jsp</result>
			<result name="success">/selectMaterialsRequisition.jsp</result>
		</action>
		
		<action name="selectDetails_MaterialsRequisition" class="materialsRequisitionAction"
			method="selectDetails">
			<result name="success">/detailsMaterialsRequisition.jsp</result>
		</action>
		
	</package>
</struts>