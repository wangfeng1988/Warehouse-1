<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="entryActions" extends="struts-default">

		<action name="add_WarehousingEntry" class="warehousingEntryAction"
			method="add">
			<result name="input">/addWarehousingEntry.jsp</result>
			<result name="success">/addWarehousingEntry.jsp</result>
		</action>

		<action name="delete_WarehousingEntry" class="warehousingEntryAction"
			method="delete">
			<result name="success">/thankyou.jsp</result>
		</action>

		<action name="select_WarehousingEntry" class="warehousingEntryAction"
			method="select">
			<result name="input">/selectWarehousingEntry.jsp</result>
			<result name="success">/selectWarehousingEntry.jsp</result>
		</action>
		
		<action name="selectDetails_WarehousingEntry" class="warehousingEntryAction"
			method="selectDetails">
			<result name="success">/detailsWarehousingEntry.jsp</result>
		</action>
		
		<action name="toExcel_WarehousingEntry" class="warehousingEntryAction"
			method="toExcel">
			<result name="success">/MakeExcel.jsp</result>
		</action>
		
	</package>
</struts>