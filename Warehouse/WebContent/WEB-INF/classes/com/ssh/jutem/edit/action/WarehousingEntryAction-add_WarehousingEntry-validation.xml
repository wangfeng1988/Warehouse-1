<!DOCTYPE validators PUBLIC
  		"-//Apache Struts//XWork Validator 1.0.2//EN"
  		"http://struts.apache.org/dtds/xwork-validator...">
<validators>
	<validator type="date">
		<param name="fieldname">entryBean.prepared_by_date</param>
		<param name="min">1900-01-01</param>
		<param name="max">2050-02-21</param>
		<message>prepared_by_date must between 1900-01-01 to 2050-02-21.</message>
	</validator>

	<validator type="int">
		<param name="fieldname">entryBean.document_number</param>
		<param name="min">1000</param>
		<param name="max">9999</param>
		<message>document_number must between 1000 to 9999 .</message>
	</validator>
	<validator type="int">
		<param name="fieldname">entryBean.original_invoice_number</param>
		<param name="min">1000</param>
		<param name="max">9999</param>
		<message>original_invoice_number must be 1000 to 9999 .</message>
	</validator>

	<validator type="requiredstring">
		<param name="fieldname">entryBean.shipping_warehouse</param>
		<message>shipping_warehouse is required.</message>
	</validator>
	<validator type="requiredstring">
		<param name="fieldname">entryBean.supplier</param>
		<message>supplier is required.</message>
	</validator>

	<validator type="double">
		<param name="fieldname">entryBean.total_number</param>
		<param name="min">0</param>
		<param name="max">99999</param>
		<message>total_number must between 0 to 99999.</message>
	</validator>
	
	<validator type="double">
		<param name="fieldname">entryBean.total_money</param>
		<param name="min">0</param>
		<param name="max">99999</param>
		<message>Ytotal_money must between 0 to 99999.</message>
	</validator>
</validators>
